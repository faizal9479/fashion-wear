<section>
  <div class="container">

    <table class="table table-bordered mt-5 mt-5 text-center" id="product-table">
      <thead>
        <tr>
          <th scope="col">No</th>
          <th scope="col">Name</th>
          <th scope="col">Mobile</th>
          <th scope="col">Email</th>
          
        </tr>
      </thead>
      <tbody>

        {{#each users}}
        <tr>
          <th scope="row">{{@key}}</th>
          <td>{{this.Name}}</td>
          <td>{{this.Mobile}}</td>
          <td>{{this.Email}}</td>
     
        </tr>
        {{/each}}

      </tbody>
    </table>
  </div>
</section>